{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/index"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        }
    ]
}